<header class="container">
  <nav>
    <% if current_user %>
      <ul>
        <li><%= current_user.email %></li>
        <li><%= link_to "Log out", logout_path, method: "delete" %></li>
      </ul>
    <% else %>
      <ul>
        <li><%= link_to "Sign Up", 'signup' %></li>
      </ul>
    <% end %>
  </nav>
</header>